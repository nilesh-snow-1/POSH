        <script><![CDATA[var TestFilterDynamic = Class.create();
TestFilterDynamic.prototype = {
    initialize: function() {

    },

    getassignedusers: function() {

        var grp = current.variables.assignment_group.sys_id;

        var gr = new GlideRecord('sys_user_grmember');
        gr.addQuery('u_assignment_group', grp);
        gr.query();

        var member = '';

        while (gr.next()) {
            member += ',' + gr.user.sys_id;
        }

        return 'sys_idIN' + member;
    },

    type: 'TestFilterDynamic'
};
]]></script>
