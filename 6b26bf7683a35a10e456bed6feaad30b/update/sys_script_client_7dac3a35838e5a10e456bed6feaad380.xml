        <condition/>
        <description>10. **Auto-Populate User Details**  &#13;
 Implement a feature to auto-populate user details without requiring a callback.</description>
        <field>u_victim</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Auto-Populate User Details</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
  if (isLoading || !newValue) return;

  g_form.getReference('u_victim', function(victim) {
    // victim is the sys_user JSON returned by the server. This a call back script
    var phone = victim && victim.mobile_phone ? victim.mobile_phone : '';
    g_form.setValue('u_victim_s_mobile_number', phone);
  });
}
]]></script>
